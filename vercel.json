{
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/shiki/api/graphql",
            "destination": "/api/shiki/graphql"
        },
        {
            "source": "/auth/callback",
            "destination": "/api/auth/callback"
        }
    ]
}
